{
  "name": "@atsu/takao",
  "version": "1.0.0",
  "type": "module",
  "description": "A basic game engine for console games with AI integration",
  "main": "dist/takao.cjs",
  "module": "dist/takao.js",
  "browser": "dist/takao.global.js",
  "types": "dist/takao.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "tsx examples/takao-engine-demo.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "test": "vitest run"
  },
  "keywords": [
    "game-engine",
    "console",
    "ai",
    "typescript"
  ],
  "author": "Tsukugi",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.0.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@atsu/atago": "file:../Atago",
    "@atsu/choukai": "file:../Choukai"
  }
}
